{
    "routes": [
      {
        "route": "/",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/consumer",
        "serve": "/consumer.html",
        "statusCode": 200
      },
      {
        "route": "/creator",
        "serve": "/creator.html",
        "statusCode": 200
      },
      {
        "route": "/styles/*",
        "serve": "/styles/*",
        "statusCode": 200
      },
      {
        "route": "/scripts/*",
        "serve": "/scripts/*",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": [
        "/styles/*",
        "/scripts/*",
        "/backend/*"
      ]
    },
    "responseOverrides": {
      "401": {
        "rewrite": "/index.html",
        "statusCode": 302
      },
      "404": {
        "rewrite": "/index.html",
        "statusCode": 302
      }
    },
    "mimeTypes": {
      ".json": "application/json",
      ".css": "text/css",
      ".js": "application/javascript"
    },
    "globalHeaders": {
      "Content-Security-Policy": "default-src 'self'; connect-src 'self' https://photosapp-insta-gca8aafdbygffjbq.canadacentral-01.azurewebsites.net; script-src 'self' 'unsafe-inline' https://fonts.googleapis.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; img-src 'self' data: https://*.blob.core.windows.net; font-src 'self' https://fonts.gstatic.com",
      "X-Frame-Options": "DENY",
      "X-Content-Type-Options": "nosniff"
    }
  }